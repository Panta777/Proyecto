    <env-entry>
        <env-entry-name>DTE_PATH</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>Z:/</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>MODO</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>OnLine/</env-entry-value>
        <!--        <env-entry-value>StandAlone/</env-entry-value> -->
    </env-entry>
    <env-entry>
        <env-entry-name>EMAIL_PDF_ACTIVO</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>false</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>EMAIL_ACTIVO</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>false</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>SMTP_HOST_NAME</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>smtp.gmail.com</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>SMTP_HOST_PORT</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>587</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>SMTP_FROM</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>panle777@gmail.com</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>SMTP_AUTH_REQ</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>true</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>SMTP_AUTH_USER</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>panle777@gmail.com</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>SMTP_AUTH_PWD</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>medrano7</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>DB_CLASSNAME</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>oracle.jdbc.driver.OracleDriver</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>DB_URL</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>jdbc:oracle:thin:@localhost:1521:orclsys</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>DB_USERNAME</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>C##PRUEBAS</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>DB_PASSWORD</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>medrano7</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>DBDATASOURCE</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>MYSQL</env-entry-value>
    </env-entry>
    <resource-ref>
        <description>DBMYSQLURL</description>
        <res-ref-name>DBMYSQLURL</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
<!--    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>-->
</web-app>
